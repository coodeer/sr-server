			<field-result name="toShortAddress" column="to_short_address" />
			<field-result name="fromToDistance" column="from_to_distance" />
		</entity-result>
	</sql-result-set-mapping>
	<!-- 调用ve_master_get_vehicle_detail 返回的bean -->
	<sql-result-set-mapping name="ReturnVehicleCommentType">
		<entity-result entity-class="com.sfc.api.mysql.entity.procedure.VehicleComment">
			<field-result name="bookingNo" column="booking_no" />
			<field-result name="id" column="id" />
			<field-result name="commentLevel" column="comment_level" />
