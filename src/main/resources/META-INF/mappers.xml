	<!-- 券查询 -->
	<sql-result-set-mapping name="ReturnTicketStatsDetailType">
		<entity-result entity-class="com.sfc.api.mysql.entity.TicketStats">
			<field-result name="id" column="id" />
			<field-result name="total" column="total" />
			<field-result name="name" column="name" />
			<field-result name="ticketAmount" column="ticket_amount" />
			<field-result name="startDate" column="start_dt" />
			<field-result name="endDate" column="end_dt" />
		</entity-result>
	</sql-result-set-mapping>

	<sql-result-set-mapping name="ReturnTicketInfoDetailType">
		<entity-result entity-class="com.sfc.api.mysql.entity.TbTicketInfo">
			<field-result name="ticketId" column="ticket_id" />
			<field-result name="ticketNo" column="ticket_no" />
			<field-result name="ticketType" column="ticket_type" />
			<field-result name="ticketName" column="ticket_name" />
			<field-result name="userId" column="user_id" />
			<field-result name="userType" column="user_type" />
			<field-result name="ticketStatus" column="ticket_status" />
			<field-result name="ticketAmount" column="ticket_amount" />
			<field-result name="startDt" column="start_dt" />
			<field-result name="endDt" column="end_dt" />
			<field-result name="createDt" column="create_dt" />
			<field-result name="updateDt" column="update_dt" />
		</entity-result>
	</sql-result-set-mapping>
</entity-mappings>