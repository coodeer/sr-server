	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence/orm http://xmlns.jcp.org/xml/ns/persistence/orm_2_0.xsd">
	<!-- 调用shenzhen.ve_vehicle_detail_get过程返回的Bean -->
	<sql-result-set-mapping name="ReturnVeVehicleInfoDetailType">
		<entity-result entity-class="com.sfc.api.mysql.entity.procedure.VeVehicleInfoDetail">
			<field-result name="carTypeId" column="cartype_id" />
			<field-result name="tel" column="tel" />
			<field-result name="veLevel" column="star_level" />
			<field-result name="name" column="name" />
			<field-result name="companyName" column="company_name" />
			<field-result name="imageUrl" column="image_url" />
			<field-result name="imageUrlTail" column="image_url_tail" />
			<field-result name="imageUrlSide" column="image_url_side" />
			<field-result name="imageUrlUser" column="image_url_user" />
			<field-result name="carBrand" column="car_brand" />
			<field-result name="carPlates" column="car_plates" />
			<field-result name="operunit" column="operunit" />
			<field-result name="longitude" column="longitude" />
			<field-result name="latitude" column="latitude" />
			<field-result name="cargoTypeList" column="cargo_type_list" />
