			<field-result name="createTime" column="createtime" />
			<field-result name="commentType" column="comment_type" />
		</entity-result>
	</sql-result-set-mapping>
	<!-- 调用ve_vehicle_info_staff_for_detail 返回的bean -->
	<sql-result-set-mapping name="ReturnVehicleInfoDetailForStaffType">
		<entity-result entity-class="com.sfc.api.mysql.entity.procedure.VehicleInfoStaffDetail">
			<field-result name="vehicleInfoId" column="vehicle_info_id" />
			<field-result name="vePlates" column="ve_plates" />
			<field-result name="modelId" column="model_id" />
			<field-result name="brandId" column="brand_id" />
			<field-result name="veProductDt" column="ve_product_dt" />
