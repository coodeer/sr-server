		</entity-result>
	</sql-result-set-mapping>
	<sql-result-set-mapping name="ReturnVoiceListDetailType">
		<entity-result entity-class="com.sfc.api.mysql.entity.procedure.VoiceListDetail">
			<field-result name="id" column="id" />
			<field-result name="filePath" column="file_path" />
			<field-result name="fileName" column="file_name" />
			<field-result name="expireTime" column="expire_time" />
			<field-result name="orderType" column="order_type" />
			<field-result name="fromAddress" column="from_address" />
			<field-result name="toAddress" column="to_address" />
			<field-result name="goodsLength" column="goods_length" />
			<field-result name="goodsWidth" column="goods_width" />
